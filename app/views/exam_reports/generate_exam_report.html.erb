<div id="content-header">
  <h1>Exam Reports</h1>
  <h3>Exam Wise Report</h3>
  <div id="app-back-button">
    <%= link_to image_tag("buttons/back.png",:border => 0), "" %>
  </div>
<div><%= image_tag("underline.jpg",width: "100%")%></div>
</div>
<div id="inner-tab-menu">
  <ul>
      <li style="width:150px"><%= link_to 'Consolidated Report',exam_reports_consolidated_report_path(@exam_group) %></li>
      <li><%= link_to 'Pdf Report','' %></li>
  </ul>
</div>
<div id="page-yield">
<div class="box">
  <div class="bread_crumb">
    <%= link_to 'Exams',  '/exams'  %> &raquo;
    <%= link_to 'Report Center', '/exam_reports/report_center' %> &raquo;
    <%= link_to 'Exam wise report', '/exam_reports/exam_wise_report' %> &raquo;
    Generated report
  </div>
  <h3 align="center"><%= @batch.name %> - <%= @exam_group.name %></h3>
  <div id="student_list">
    <%= render 'exam_wise_student_list' %>
  </div>
  <div id="exam_wise_report">
    <%= render 'exam_wise_report' %>
  </div>
</div></div>